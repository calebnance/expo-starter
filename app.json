{
  "expo": {
    "name": "Expo Starter",
    "slug": "expo-starter",
    "privacy": "hidden",
    "version": "0.0.1",
    "platforms": ["android", "ios", "web"],
    "githubUrl": "https://github.com/calebnance/expo-starter",
    "orientation": "portrait",
    "primaryColor": "#30778c",
    "icon": "src/assets/icon.png",
    "splash": {
      "backgroundColor": "#30778c",
      "image": "src/assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
