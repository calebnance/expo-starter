{
  "name": "expo-starter",
  "version": "1.0.0",
  "description": "starter template for expo/react native app",
  "author": "Caleb Nance",
  "license": "MIT",
  "keywords": [
    "expo",
    "react native",
    "react navigation"
  ],
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "dev": "expo start",
    "start": "expo start",
    "lint": "eslint ./src App.js",
    "android": "expo start --android",
    "android-bundle": "eas build --platform android",
    "ios": "expo start --ios",
    "ios-bundle": "eas build --platform ios",
    "web": "expo start --web",
    "web-build": "expo build:web",
    "eject": "expo eject"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "^7",
    "@react-navigation/native": "^7",
    "@react-navigation/native-stack": "^7",
    "expo": "^53.0.11",
    "expo-asset": "~11.1.5",
    "expo-font": "~13.3.1",
    "expo-splash-screen": "~0.30.9",
    "expo-updates": "~0.28.14",
    "prop-types": "^15.8.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "babel-eslint": "^10.1.0",
    "babel-preset-expo": "~13.0.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-local-rules": "^3.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "prettier": "^2.8.8"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "eslintIgnore": [
    "babel.config.js"
  ],
  "engines": {
    "node": ">=22.16.0"
  }
}
