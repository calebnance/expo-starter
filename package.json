{
  "name": "expo-starter",
  "version": "0.0.1",
  "description": "quick prototyping starter template for expo/react native",
  "author": "Caleb Nance",
  "license": "MIT",
  "keywords": [
    "expo",
    "react native",
    "react navigation"
  ],
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "dev": "expo start",
    "start": "expo start",
